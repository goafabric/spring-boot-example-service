_Release_namespace=example
_Values_root_name=country-service
_Values_app_name=${_Values_root_name}-application

_Values_image_repository=goafabric/spring-boot-exampleservice
_Values_image_tag=1.0.5-SNAPSHOT
_Values_image_pullPolicy=IfNotPresent #Alwas

_Values_service_type=ClusterIP
_Values_service_port=8080
_Values_service_nodePort_dev=30900

_Values_resources_requests_memory=256M
_Values_resources_requests_cpu=250m

_Values_readinessProbe_initialDelaySeconds=10

_Values_command=org.goafabric.spring.boot.exampleservice.Application

_Values_linkerd_inject=enabled